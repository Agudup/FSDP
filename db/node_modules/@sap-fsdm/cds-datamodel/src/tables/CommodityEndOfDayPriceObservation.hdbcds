namespace "sap"."fsdm";

using "sap"."fsdm"::"Commodity";
using "sap"."fsdm"::"Exchange";

entity "CommodityEndOfDayPriceObservation" {
    key "PriceDataProvider"             : String(128);
    key "PriceSeriesType"               : String(40);
    key "_Commodity"                    : association to Commodity { CommodityID }         not null;
    key "_Exchange"                     : association to Exchange { MarketIdentifierCode } not null;
    key "BusinessValidFrom"             : LocalDate;
    key "BusinessValidTo"               : LocalDate;
        "SystemValidFrom"               : UTCTimestamp                                     not null;
        "SystemValidTo"                 : UTCTimestamp                                     not null;
        "Close"                         : Decimal(34, 6);
        "Currency"                      : String(3);
        "High"                          : Decimal(34, 6);
        "Low"                           : Decimal(34, 6);
        "Open"                          : Decimal(34, 6);
        "PriceNotationForm"             : String(40);
        "Quantity"                      : Decimal(34, 6);
        "Unit"                          : String(10);
        "SourceSystemID"                : String(128);
        "ChangeTimestampInSourceSystem" : UTCTimestamp;
        "ChangingUserInSourceSystem"    : String(128);
        "ChangingProcessType"           : String(40);
        "ChangingProcessID"             : String(128);
}
technical configuration {
    column store;
};

entity "CommodityEndOfDayPriceObservation_Historical" {
    "PriceDataProvider"             : String(128)                                      not null;
    "PriceSeriesType"               : String(40)                                       not null;
    "_Commodity"                    : association to Commodity { CommodityID }         not null;
    "_Exchange"                     : association to Exchange { MarketIdentifierCode } not null;
    "BusinessValidFrom"             : LocalDate                                        not null;
    "BusinessValidTo"               : LocalDate                                        not null;
    "SystemValidFrom"               : UTCTimestamp                                     not null;
    "SystemValidTo"                 : UTCTimestamp                                     not null;
    "Close"                         : Decimal(34, 6);
    "Currency"                      : String(3);
    "High"                          : Decimal(34, 6);
    "Low"                           : Decimal(34, 6);
    "Open"                          : Decimal(34, 6);
    "PriceNotationForm"             : String(40);
    "Quantity"                      : Decimal(34, 6);
    "Unit"                          : String(10);
    "SourceSystemID"                : String(128);
    "ChangeTimestampInSourceSystem" : UTCTimestamp;
    "ChangingUserInSourceSystem"    : String(128);
    "ChangingProcessType"           : String(40);
    "ChangingProcessID"             : String(128);
}
technical configuration {
    column store;
};