namespace "sap"."fsdm";

entity "TradeClass" {
    key "TradeClass"                     : String(40);
    key "TradeClassificationSystem"      : String(128);
    key "BusinessValidFrom"              : UTCTimestamp;
    key "BusinessValidTo"                : UTCTimestamp;
        "SystemValidFrom"                : UTCTimestamp not null;
        "SystemValidTo"                  : UTCTimestamp not null;
        "TradeClassDescription"          : String(256);
        "TradeClassificationDescription" : String(256);
        "SourceSystemID"                 : String(128);
        "ChangeTimestampInSourceSystem"  : UTCTimestamp;
        "ChangingUserInSourceSystem"     : String(128);
        "ChangingProcessType"            : String(40);
        "ChangingProcessID"              : String(128);
}
technical configuration {
    column store;
};

entity "TradeClass_Historical" {
    "TradeClass"                     : String(40)   not null;
    "TradeClassificationSystem"      : String(128)  not null;
    "BusinessValidFrom"              : UTCTimestamp not null;
    "BusinessValidTo"                : UTCTimestamp not null;
    "SystemValidFrom"                : UTCTimestamp not null;
    "SystemValidTo"                  : UTCTimestamp not null;
    "TradeClassDescription"          : String(256);
    "TradeClassificationDescription" : String(256);
    "SourceSystemID"                 : String(128);
    "ChangeTimestampInSourceSystem"  : UTCTimestamp;
    "ChangingUserInSourceSystem"     : String(128);
    "ChangingProcessType"            : String(40);
    "ChangingProcessID"              : String(128);
}
technical configuration {
    column store;
};